	GetIni "KanmusuFormationMode", "SecondFleetName", SecondFleetName, "\"第二艦隊\""
	GetIni "KanmusuFormationMode", "ThirdFleetName", ThirdFleetName, "\"ボス支援\""
	GetIni "KanmusuFormationMode", "FourthFleetName", FourthFleetName, "\"道中支援\""	
